Object subclass: #AlgoChat	instanceVariableNames: 'baseDeUsuarios salasDeChat mensajeDirecto'	classVariableNames: ''	poolDictionaries: ''	category: 'AlgoChat'!!AlgoChat methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/14/2019 16:24'!publicarMensaje: nuevoMensaje enCanal: nombreDelCanal 	salasDeChat publicarMensaje: nuevoMensaje enSala: nombreDelCanal.! !!AlgoChat methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 02:10'!getNotificacionesDeUsuario: nombreDelUsuario resumidasALongitud: caracteresMaximosDeCadaMensaje	|notificacionesResumidas|		notificacionesResumidas := baseDeUsuarios getNotificacionesResumidasDe: nombreDelUsuario 														  conLongitudMaxima: caracteresMaximosDeCadaMensaje. 															^ notificacionesResumidas.! !!AlgoChat methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 01:39'!getCantidadUsuarios	^ baseDeUsuarios getCantidadUsuarios.! !!AlgoChat methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/14/2019 16:27'!mensajesDelCanal: nombreDelCanal	|mensajes|	mensajes := salasDeChat mensajesDeLaSala: nombreDelCanal.		^mensajes.! !!AlgoChat methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/15/2019 16:16'!reanudarPalabrasDeCanal: nombreDelCanal paraUsuario: nombreDeUsuario 	|usuario|	usuario := baseDeUsuarios buscar: nombreDeUsuario .	usuario reanudarPalabrasDeCanal: nombreDelCanal. ! !!AlgoChat methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/14/2019 01:40'!agregarPalabraClave: unaPalabra alUsuario: nombreDelUsuario	|usuario|	usuario := baseDeUsuarios buscar: nombreDelUsuario.	usuario agregarPalabraClave: unaPalabra.! !!AlgoChat methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/14/2019 16:41'!mensajesDelCanal: nombreDelCanal resumidosALongitud: longitudDeCadaMensaje	|resumen|	resumen := salasDeChat mensajesDeLaSala: nombreDelCanal resumidosALongitud: longitudDeCadaMensaje.	^resumen.! !!AlgoChat methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 01:47'!agregarUsuarioConNombre: nombreDelUsuario	baseDeUsuarios agregar: nombreDelUsuario.! !!AlgoChat methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 02:02'!enviarMensaje: unNuevoMensaje aUsuario: usuarioReceptor 	|usuario|	usuario := baseDeUsuarios buscar: usuarioReceptor.	mensajeDirecto enviarMensajePrivado: unNuevoMensaje alUsuario: usuario.	! !!AlgoChat methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/14/2019 16:27'!getMensajesDeConversacion: nombreDeLaConversacion	|mensajes|	mensajes := salasDeChat mensajesDeLaSala: nombreDeLaConversacion.		 ^ mensajes.! !!AlgoChat methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 02:16'!agregarCanalConNombre: nuevoCanal	salasDeChat agregarCanal: nuevoCanal.! !!AlgoChat methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 02:04'!getNotificacionesDeUsuario: nombreDelUsuario	|notificaciones|		notificaciones := baseDeUsuarios getNotificacionesDe: nombreDelUsuario.		^ notificaciones.! !!AlgoChat methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/14/2019 16:24'!publicarMensaje: nuevoMensaje enConversacion: nombreDeConversacion 	salasDeChat publicarMensaje: nuevoMensaje enSala: nombreDeConversacion. ! !!AlgoChat methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/15/2019 16:00'!pausarPalabrasDeCanal: nombreDelCanal paraUsuario: nombreDeUsuario 	|usuario|	usuario := baseDeUsuarios buscar: nombreDeUsuario .	usuario pausarMensajesDeLCanal: nombreDelCanal.		"salasDeChat pausarPalabrasDelCanal: nombreDelCanal paraUsuario: nombreDeUsuario".! !!AlgoChat methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 02:53'!crearConversacionConUsuarios: nombreDeLosUsuarios	|usuariosDelGrupo|		usuariosDelGrupo := baseDeUsuarios buscarUsuarios: nombreDeLosUsuarios.	salasDeChat crearGrupoCon: usuariosDelGrupo yConNombre: nombreDeLosUsuarios.! !!AlgoChat methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 01:47'!getCantidadDeCanales	^ (salasDeChat getCantidadDeCanales).! !!AlgoChat methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/14/2019 16:29'!agregarUsuario: nombreDelUsuario alCanal: nombreDelCanal	|usuario|	usuario := baseDeUsuarios buscar: nombreDelUsuario.		salasDeChat agregarAlCanal:nombreDelCanal elUsuario: usuario.! !!AlgoChat methodsFor: 'initialization' stamp: 'veronicaleguizamon 9/13/2019 01:55'!initialize 	baseDeUsuarios := BaseDeUsuarios new.	salasDeChat := SalaDeChat new.	mensajeDirecto := Mensajeria new.! !Object subclass: #BaseDeUsuarios	instanceVariableNames: 'usuarios'	classVariableNames: ''	poolDictionaries: ''	category: 'AlgoChat'!!BaseDeUsuarios methodsFor: 'initialization' stamp: 'veronicaleguizamon 9/13/2019 01:34'!initialize 	usuarios := OrderedCollection new.! !!BaseDeUsuarios methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 02:52'!buscarUsuarios: nombreDeLosUsuarios	|listaDeNombres usuariosBuscados|	listaDeNombres := nombreDeLosUsuarios splitOn: ','.		usuariosBuscados := listaDeNombres collect: [ :nombre | self buscar: nombre ].		^ usuariosBuscados.! !!BaseDeUsuarios methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 01:34'!getCantidadUsuarios	^ (usuarios size).! !!BaseDeUsuarios methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/14/2019 18:33'!getNotificacionesDe: unUsuario	|usuario notificaciones|	usuario := self buscar: unUsuario.	notificaciones := usuario getNotificaciones.		^notificaciones.! !!BaseDeUsuarios methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/14/2019 18:30'!buscar: usuarioBuscado	|usuarioEncontrado existeElUsuario |		existeElUsuario := usuarios anySatisfy: [ :usuario | usuario coincideNombre: usuarioBuscado ].	 	(existeElUsuario) ifFalse: [self agregar: usuarioBuscado].							usuarioEncontrado := usuarios detect: [ :usuario | usuario coincideNombre: usuarioBuscado ].      ^ usuarioEncontrado.! !!BaseDeUsuarios methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 01:51'!agregar: nombreDelNuevoUsuario	|usuarioNuevo|	usuarioNuevo := Usuario conNombre: nombreDelNuevoUsuario.		usuarios add: usuarioNuevo.! !!BaseDeUsuarios methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 02:11'!getNotificacionesResumidasDe: nombreDelUsuario conLongitudMaxima: longitudMaximaDelMensaje 	|usuario resumen|	usuario := self buscar: nombreDelUsuario.		resumen := usuario getNotificacionesResumidasConLongitud: longitudMaximaDelMensaje.		^ resumen.! !Object subclass: #Canal	instanceVariableNames: 'nombre mensajes usuarios mensajeria usuariosParaEnviarMensajes usuariosPausados'	classVariableNames: ''	poolDictionaries: ''	category: 'AlgoChat'!!Canal methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 02:32'!getMensajesConLongitud: longitudMaxima	|resumen|	resumen := mensajes getMensajesConLongitud: longitudMaxima.	^resumen.! !!Canal methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 02:18'!setNombre: nuevoNombre	nombre := nuevoNombre.! !!Canal methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/15/2019 16:18'!agregarUsuario: unUsuario	usuarios add: unUsuario.! !!Canal methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 02:22'!coincideNombre: nombreDelCanal	^ (nombre=nombreDelCanal ).! !!Canal methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/14/2019 18:33'!getNotificaciones	|mensajesDelCanal|	mensajesDelCanal := mensajes getNotificaciones.	^mensajesDelCanal.! !!Canal methodsFor: 'initialization' stamp: 'veronicaleguizamon 9/15/2019 16:06'!nombre	^nombre.! !!Canal methodsFor: 'initialization' stamp: 'veronicaleguizamon 9/15/2019 14:49'!initialize 	nombre := ''.	mensajes := Mensaje new.	usuarios := OrderedCollection new.	mensajeria := Mensajeria new.	usuariosParaEnviarMensajes := OrderedCollection new.! !!Canal methodsFor: 'initialization' stamp: 'veronicaleguizamon 9/15/2019 16:13'!publicarMensaje: nuevoMensaje	mensajeria revisarPalabrasDelCanal: (self nombre) conElMensaje: nuevoMensaje yLosUsuarios: usuarios. 	"mensajeria revisarPalabrasClavesDe:nuevoMensaje yEnviarMensajesA: usuarios."	mensajes agregar: nuevoMensaje.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Canal class	instanceVariableNames: ''!!Canal class methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 02:17'!conNombre: nuevoCanal	|canal|	canal := Canal new.	canal setNombre: nuevoCanal.		^canal.! !Object subclass: #Grupo	instanceVariableNames: 'nombre usuarios mensajes mensajeria'	classVariableNames: ''	poolDictionaries: ''	category: 'AlgoChat'!!Grupo methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 02:58'!setNombre: nombreDelGrupo	nombre := nombreDelGrupo .! !!Grupo methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 03:05'!publicarMensaje: mensajeParaPublicar	mensajeria enviarMensaje: mensajeParaPublicar  aLosUsuarios: usuarios.	mensajes agregar: mensajeParaPublicar  ! !!Grupo methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 02:58'!setUsuariosQuePartipan: usuariosParticipantes	usuarios  := usuariosParticipantes .! !!Grupo methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 03:03'!coincideNombre: nombreParaProbar	^ (nombre = nombreParaProbar  ).! !!Grupo methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/14/2019 18:33'!getNotificaciones	|mensajesDelGrupo|	mensajesDelGrupo  := mensajes getNotificaciones.	 ^mensajesDelGrupo .! !!Grupo methodsFor: 'initialization' stamp: 'veronicaleguizamon 9/14/2019 16:34'!initialize	nombre := ''.	usuarios := OrderedCollection new.	mensajeria := Mensajeria new.	mensajes := Mensaje new.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Grupo class	instanceVariableNames: ''!!Grupo class methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 02:57'!conNombre: nombreDelGrupo yParticipantes: usuariosQueParticipan	|grupo|	grupo := Grupo new.	grupo setNombre: nombreDelGrupo.	grupo setUsuariosQuePartipan: usuariosQueParticipan.		^ grupo.! !Object subclass: #Mensaje	instanceVariableNames: 'mensajes'	classVariableNames: ''	poolDictionaries: ''	category: 'AlgoChat'!!Mensaje methodsFor: 'initialization' stamp: 'veronicaleguizamon 9/13/2019 02:00'!initialize 		mensajes := OrderedCollection new.! !!Mensaje methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 16:30'!getMensajesConLongitud: numeroDeCaracteresMaximos	|mensajesResumidos resumenFinal|	mensajesResumidos := mensajes collect: [ :contenido | ((contenido size) < numeroDeCaracteresMaximos) 														ifFalse: [(contenido first: numeroDeCaracteresMaximos),'...']														ifTrue: [(contenido)]														].		resumenFinal := mensajesResumidos joinUsing: ' | '.		^ resumenFinal.! !!Mensaje methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 02:01'!agregar: unNuevoMensaje	mensajes add: unNuevoMensaje.! !!Mensaje methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/14/2019 18:33'!getNotificaciones	|notificaciones|		notificaciones := mensajes joinUsing: ' | '.		^notificaciones.! !Object subclass: #Mensajeria	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'AlgoChat'!!Mensajeria methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 03:06'!enviarMensaje: unMensaje aLosUsuarios: usuarios	usuarios do: [ :usuario | usuario enviarMensaje: unMensaje  ].! !!Mensajeria methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/15/2019 14:23'!revisarPalabrasClavesDe:nuevoMensaje yEnviarMensajesA:usuarios	|mencionados|		mencionados := usuarios select: [:usuario | ((usuario buscarMencion:nuevoMensaje) | (usuario buscarPalabraClave: nuevoMensaje))].		self enviarMensaje: nuevoMensaje  aLosUsuarios: mencionados.! !!Mensajeria methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/15/2019 16:03'!revisarPalabrasDelCanal: nombreDelCanal conElMensaje:nuevoMensaje yLosUsuarios:usuarios	|mencionados|		mencionados := usuarios select: [:usuario | ((usuario buscarMencion:nuevoMensaje) | (usuario buscarPalabraClave: nuevoMensaje delCanal: nombreDelCanal) )].		self enviarMensaje: nuevoMensaje  aLosUsuarios: mencionados.! !!Mensajeria methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/15/2019 15:46'!revisarMensaje:nuevoMensaje deMencionesDe:usuarios yPalabrasClave: usuariosConPalabrasClave	! !!Mensajeria methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 01:58'!enviarMensajePrivado: unMensaje alUsuario: unUsuario 	unUsuario enviarMensaje: unMensaje.! !Object subclass: #PalabrasClaves	instanceVariableNames: 'palabras canalesPausados'	classVariableNames: ''	poolDictionaries: ''	category: 'AlgoChat'!!PalabrasClaves methodsFor: 'initialization' stamp: 'veronicaleguizamon 9/15/2019 17:25'!initialize 	palabras := OrderedCollection new.	canalesPausados := OrderedCollection new.! !!PalabrasClaves methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/15/2019 17:27'!agregar: palabra	palabras add: palabra.! !!PalabrasClaves methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/15/2019 17:29'!pausarCanal: nombreDelCanal	canalesPausados add: nombreDelCanal.! !Object subclass: #SalaDeChat	instanceVariableNames: 'canales grupos salas'	classVariableNames: ''	poolDictionaries: ''	category: 'AlgoChat'!!SalaDeChat methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/14/2019 16:28'!mensajesDeLaSala: nombre resumidosALongitud: longitudMaxima 	|sala resumenDeLosMensajes |	sala := self buscar: nombre.		resumenDeLosMensajes := sala getMensajesConLongitud: longitudMaxima.		^ resumenDeLosMensajes.! !!SalaDeChat methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/14/2019 18:33'!mensajesDeLaSala: nombreDeLaSala	|sala mensajes|			sala := self buscar: nombreDeLaSala.		mensajes := sala getNotificaciones.		^mensajes.		! !!SalaDeChat methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 03:21'!getCantidadDeCanales	|cantidadDeCanales|	cantidadDeCanales :=	salas count: [ :sala | sala = Canal ].	^cantidadDeCanales ! !!SalaDeChat methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/14/2019 01:39'!getCantidadDeSalas 	|cantidadDeSalas|	cantidadDeSalas :=	salas count: [ :sala | sala = Grupo ].	^cantidadDeSalas ! !!SalaDeChat methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/14/2019 18:33'!getMensajesDeLaConversacion: nombreDeLaConversacion	|grupo mensajes|	grupo := self buscar: nombreDeLaConversacion.		mensajes := grupo getNotificaciones.	^mensajes.! !!SalaDeChat methodsFor: 'agregar' stamp: 'veronicaleguizamon 9/14/2019 16:29'!agregarAlCanal: nombreDelCanal elUsuario: unUsuario	|canal|	canal := self buscar: nombreDelCanal.	canal agregarUsuario: unUsuario.! !!SalaDeChat methodsFor: 'agregar' stamp: 'veronicaleguizamon 9/13/2019 03:22'!crearGrupoCon: usuariosQueParticipan yConNombre: nombreDelGrupo	|nuevoGrupo|		nuevoGrupo := Grupo conNombre: nombreDelGrupo yParticipantes: usuariosQueParticipan.	salas add: nuevoGrupo .! !!SalaDeChat methodsFor: 'agregar' stamp: 'veronicaleguizamon 9/13/2019 03:22'!agregarCanal: nombreNuevoCanal	|nuevoCanal|		nuevoCanal := Canal conNombre: nombreNuevoCanal.	salas add: nuevoCanal.! !!SalaDeChat methodsFor: 'buscar' stamp: 'veronicaleguizamon 9/13/2019 03:22'!buscar: nombreDeLaSala	|sala|	sala := salas detect: [ :salaActual | salaActual coincideNombre: nombreDeLaSala  ].	^ sala.! !!SalaDeChat methodsFor: 'initialization' stamp: 'veronicaleguizamon 9/13/2019 03:23'!initialize 		salas := OrderedCollection new.! !!SalaDeChat methodsFor: 'publicar' stamp: 'veronicaleguizamon 9/14/2019 16:24'!publicarMensaje: nuevoMensaje enSala: nombreDeLaConversacion	|sala|	sala   := self buscar: nombreDeLaConversacion.	sala publicarMensaje: nuevoMensaje! !Object subclass: #Usuario	instanceVariableNames: 'nombre notificaciones palabrasClave canalesPausadosParaPalabras'	classVariableNames: ''	poolDictionaries: ''	category: 'AlgoChat'!!Usuario methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 02:31'!getNotificacionesResumidasConLongitud: numeroMaximoDeCaracteres	|resumen|	resumen := notificaciones getMensajesConLongitud: numeroMaximoDeCaracteres.		^ resumen.! !!Usuario methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 01:57'!coincideNombre: nombreParaComparar	^( nombre = nombreParaComparar ).! !!Usuario methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/15/2019 17:26'!agregarPalabraClave: palabraClave 	palabrasClave agregar: palabraClave.	"palabrasClave add:palabraClave"! !!Usuario methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 17:15'!buscarMencion: mensaje	^ (((mensaje findString: ('@',self nombre)) > 0)). ! !!Usuario methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/15/2019 16:15'!canalBloqueado: nombreDelCanal	|estaBloqueado|		estaBloqueado := canalesPausadosParaPalabras anySatisfy: [ :canal | canal = nombreDelCanal  ].		^estaBloqueado.	! !!Usuario methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/15/2019 16:20'!buscarPalabraClave: nuevoMensaje delCanal: nombreDelCanal   |estaLaPalabra|	(self canalBloqueado: nombreDelCanal) ifFalse: [																	estaLaPalabra := self buscarPalabraClave: nuevoMensaje.																	^ estaLaPalabra .																].					^ false.! !!Usuario methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/15/2019 16:17'!reanudarPalabrasDeCanal: nombreDelCanal	 ! !!Usuario methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 01:59'!enviarMensaje: unNuevoMensaje	notificaciones agregar: unNuevoMensaje. ! !!Usuario methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 01:52'!setNombre: nuevoNombre	nombre := nuevoNombre .! !!Usuario methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/14/2019 18:33'!getNotificaciones	|textoDeLasNotificaciones|	textoDeLasNotificaciones := notificaciones getNotificaciones.		^ textoDeLasNotificaciones .! !!Usuario methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/15/2019 14:32'!buscarPalabraClave: mensaje	|esta|	esta := palabrasClave anySatisfy: [ :palabra | (((mensaje findString: palabra) > 0)) ].		^esta.! !!Usuario methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/15/2019 17:29'!pausarMensajesDeLCanal: nombreDelCanal	"canalesPausadosParaPalabras add:nombreDelCanal."	palabrasClave pausarCanal: nombreDelCanal. ! !!Usuario methodsFor: 'accessing' stamp: 'veronicaleguizamon 9/13/2019 17:15'!nombre	^nombre.! !!Usuario methodsFor: 'initialization' stamp: 'veronicaleguizamon 9/15/2019 17:26'!initialize	nombre := ''.	notificaciones := Mensaje new.	"palabrasClave := OrderedCollection new."	canalesPausadosParaPalabras := OrderedCollection new.	palabrasClave := PalabrasClaves new.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Usuario class	instanceVariableNames: ''!!Usuario class methodsFor: 'as yet unclassified' stamp: 'veronicaleguizamon 9/13/2019 01:52'!conNombre: unNombre	|usuario|	usuario := self new.	usuario setNombre: unNombre.		^usuario.! !